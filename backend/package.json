{
  "name": "flavours-backend",
  "version": "1.0.0",
  "description": "Flavours Creator Platform - Backend Services",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/server-simple.ts",
    "dev:production": "tsx watch src/server-production.ts",
    "dev:minimal": "tsx watch src/server-minimal.ts",
    "dev:full": "tsx watch src/index.ts",
    "build": "tsc",
    "build:simple": "tsc src/server-simple.ts --outDir dist --target es2020 --module commonjs --esModuleInterop --allowSyntheticDefaultImports",
    "build:production": "tsc src/server-production.ts --outDir dist --target es2020 --module commonjs --esModuleInterop --allowSyntheticDefaultImports",
    "build:minimal": "tsc src/server-minimal.ts --outDir dist --target es2020 --module commonjs --esModuleInterop --allowSyntheticDefaultImports",
    "start": "node dist/server-simple.js",
    "start:production": "node dist/server-production.js",
    "start:minimal": "node dist/server-minimal.js",
    "start:full": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "docker:build": "docker build -t flavours-backend .",
    "docker:run": "docker run -p 3001:3001 flavours-backend"
  },
  "keywords": [
    "creator",
    "monetization",
    "subscription",
    "platform"
  ],
  "author": "Flavours Platform",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/compute": "^6.3.0",
    "@google-cloud/logging": "^11.2.1",
    "@google-cloud/monitoring": "^5.3.0",
    "@google-cloud/secret-manager": "^6.1.0",
    "@google-cloud/sql": "^0.21.0",
    "@google-cloud/storage": "^7.5.0",
    "@sentry/node": "^7.77.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/nodemailer": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dd-trace": "^3.12.0",
    "dompurify": "^3.0.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-http-context": "^1.2.4",
    "express-mongo-sanitize": "^2.2.0",
    "express-pino-logger": "^7.0.0",
    "express-prometheus-middleware": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "express-request-id": "^1.4.0",
    "express-session": "^1.17.3",
    "express-slow-down": "^2.0.1",
    "express-status-monitor": "^1.3.4",
    "express-timeout-handler": "^2.0.0",
    "express-validator": "^7.0.1",
    "ffmpeg-static": "^5.2.0",
    "firebase-admin": "^11.11.0",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsdom": "^23.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "newrelic": "^13.3.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "pino": "^8.16.2",
    "pino-http": "^8.5.0",
    "pino-pretty": "^10.2.3",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^4.6.10",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.4",
    "stripe": "^14.8.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "tsx": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
